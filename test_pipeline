// this is a test pipeline
pipeline {
  agent none
  stages {
    stage("hello world") {
      steps {
        sh abc
        sh "hello world"
	checkout([repo_name: "hello world", clone_depth: 20])
      }
    }
    stage("Script") {
      steps {
        script {
          def foo = bar
	  sendTrackingEvents(arg1, 3 + 4 * 7, false)
	  mapFunc(param1: val, param2: 4)
        }
      }
    }
  }
}
